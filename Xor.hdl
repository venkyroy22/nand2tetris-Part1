/**
 * Exclusive-or gate:
 * if ((a and Not(b)) or (Not(a) and b)) out = 1, else out = 0
 */
CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Not(in=b, out=Andb);
    Not(in=a, out=Anda);

    And(a=a, b=Andb, out=Ora);
    And(a=Anda, b=b, out=Orb);

    Or(a=Ora, b=Orb, out=out);
}